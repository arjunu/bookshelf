{
  "moduleFileExtensions": [
    "js",
    "jsx"
  ],
  "moduleDirectories": [
    "node_modules"
  ],
  "moduleNameMapper": {
    "\\.(css|less)$": "identity-obj-proxy"
  },
  "setupFiles": [
    "./setupFile.js",
    "jest-localstorage-mock"
  ],
  "collectCoverageFrom": [
    "lib/**/*.{js,jsx}",
    "!**/node_modules/**",
    "!**/vendor/**"
  ],
  "testEnvironment": "node",
  "transform": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./fileTransformer.js",
    "^.+\\.js$": "babel-jest"
  }
}